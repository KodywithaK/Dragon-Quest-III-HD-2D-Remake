      "Txt_Talk_DreamDesert_Out_MAIN_0_ACTOR_0110_010_2": {
        "SelfId": "Txt_Talk_DreamDesert_Out_MAIN_0_ACTOR_0110_010_2",
        "Text": "どうやら　<RUBY>気[き]をうしなっているようだ。"
