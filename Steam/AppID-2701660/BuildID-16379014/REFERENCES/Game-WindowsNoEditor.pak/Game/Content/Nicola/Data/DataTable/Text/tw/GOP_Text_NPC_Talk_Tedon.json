[
  {
    "Type": "DataTable",
    "Name": "GOP_Text_NPC_Talk_Tedon",
    "Class": "UScriptClass'DataTable'",
    "Flags": "RF_Public | RF_Standalone | RF_Transactional | RF_WasLoaded | RF_LoadCompleted",
    "Properties": {
      "RowStruct": {
        "ObjectName": "ScriptStruct'GOP_Text_CommonTable'",
