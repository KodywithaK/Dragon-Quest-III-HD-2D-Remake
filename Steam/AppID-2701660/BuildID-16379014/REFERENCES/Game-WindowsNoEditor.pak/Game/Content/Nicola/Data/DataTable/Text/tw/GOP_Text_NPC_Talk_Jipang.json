      "Txt_Talk_Jipang_BattleRoad_Arena_SUB_0_BATTLE_END_0110_010_9": {
        "SelfId": "Txt_Talk_Jipang_BattleRoad_Arena_SUB_0_BATTLE_END_0110_010_9",
