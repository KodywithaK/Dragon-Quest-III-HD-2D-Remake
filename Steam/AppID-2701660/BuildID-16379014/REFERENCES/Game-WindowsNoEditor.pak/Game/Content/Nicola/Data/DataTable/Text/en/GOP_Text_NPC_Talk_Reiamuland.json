      "Txt_Talk_Reiamuland_Out_MAIN_0_ACTOR_0110_030_19": {
        "SelfId": "Txt_Talk_Reiamuland_Out_MAIN_0_ACTOR_0110_030_19",
        "Text": "Venture forth and do what must be done..."
      },
